{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "layout": {
      "contentSize": "840px",
      "wideSize": "1100px"
    },
    "color": {
      "palette": [
        {
          "slug": "primary",
          "color": "#525ddc",
          "name": "Primary"
        }
      ],
      "custom": false,
      "customGradient": false,
      "defaultPalette": false,
      "defaultGradients": false
    },

    "spacing": {
      "padding": false,
      "units": ["px", "%", "em", "rem", "vw", "vh"]
    },
    "typography": {
      "customFontSize": false,
      "fontSizes": [
        {
          "slug": "normal",
          "size": "1em",
          "name": "Normal"
        },
        {
          "slug": "large",
          "size": "1.5em",
          "name": "Large"
        }
      ]
    }
  }
}
